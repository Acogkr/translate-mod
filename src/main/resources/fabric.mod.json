{
  "schemaVersion": 1,
  "id": "translatemod",
  "version": "${version}",
  "name": "TranslateMod",
  "description": "Google Gemini API를 활용한 마인크래프트 실시간 채팅 번역 모드",
  "authors": [
    "Acog"
  ],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/translatemod/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "kr.acog.translatemod.TranslateModDataGenerator"
    ],
    "client": [
      "kr.acog.translatemod.TranslateModClient"
    ],
    "main": [
      "kr.acog.translatemod.Translatemod"
    ]
  },
  "mixins": [
    "translatemod.mixins.json",
    {
      "config": "translatemod.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": ">=1.21.1"
  },
  "suggests": {
    "fabric-api": "*"
  }
}